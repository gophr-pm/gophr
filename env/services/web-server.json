{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "web-server-service",
    "namespace": "gophr"
  },
  "spec": {
    "ports": [
      {
        "port": 80,
        "targetPort": 80,
        "protocol": "TCP"
      },
      {
        "port": 443,
        "targetPort": 443,
        "protocol": "TCP"
      }
    ],
    "selector": {
      "tier": "frontend",
      "role": "web-server"
    }
  }
}
