    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.10.0",
    "imagemin": "^4.0.0",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.0",
