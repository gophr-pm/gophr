{
  "name": "gophr",
  "private": true,
  "version": "1.0.0",
  "description": "An end-to-end package management solution for Go.",
  "scripts": {
    "clean": "node ./build/scripts/web-clean",
    "assets:dev": "node ./build/scripts/web-assets-dev",
    "webpack:dev": "NODE_ENV=dev ./node_modules/webpack/bin/webpack.js --config ./build/webpack.config.js --watch --quiet",
    "dev": "npm run clean && node ./node_modules/concurrently/src/main.js --kill-others \"npm run assets:dev\" \"npm run webpack:dev\"",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js \"test/**/*@(.js|.jsx)\"",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "istanbul cover "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skeswa/gophr.git"
  },
  "keywords": [
    "go",
    "gopher",
    "gopm",
    "package",
    "manager"
  ],
  "author": "Sandile Keswa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skeswa/gophr/issues"
  },
  "homepage": "https://github.com/skeswa/gophr#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.4",
    "concurrently": "^2.0.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.10.0",
    "imagemin": "^4.0.0",
    "istanbul": "^0.4.3",
    "jsdom": "^8.4.0",
    "mocha": "^2.4.5",
    "postcss": "^5.0.19",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "react": "^15.0.1",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.8.1",
    "typescript": "^1.8.7",
    "url-loader": "^0.5.7",
    "watch": "^0.17.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "material-ui": "^0.14.4",
    "postcss-neat": "^2.5.2",
    "react": "^15.0.1",
    "react-addons-pure-render-mixin": "^15.0.1",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-router": "^2.4.0",
    "react-tap-event-plugin": "^1.0.0"
  }
}
